class Main {
   public static void main(String[] args) {
 
    for (int i = 1; i <= 10; i++){
      if (i > 5){
        break;
      }
      System.out.println("i = " + i);
     }       
 
    for (int j = 0; j < 10; j++){
     if (j == 2 || j == 8){
       continue;
      }
     System.out.println("j = " + j);
    } 
 
  }
}
